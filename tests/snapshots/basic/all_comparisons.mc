// Test all comparison operators
print($"{5 < 10}");
print($"{10 < 5}");
print($"{5 <= 5}");
print($"{5 <= 4}");
print($"{10 > 5}");
print($"{5 > 10}");
print($"{5 >= 5}");
print($"{4 >= 5}");
print($"{5 == 5}");
print($"{5 == 6}");
print($"{5 != 6}");
print($"{5 != 5}");

// Float comparisons
print($"{3.14 < 3.15}");
print($"{3.14 > 3.13}");
print($"{3.14 == 3.14}");
