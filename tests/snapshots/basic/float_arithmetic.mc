// Test float arithmetic operations

// Basic operations
let a = 5.5;
let b = 2.5;

print(a + b);  // Addition
print(a - b);  // Subtraction
print(a * b);  // Multiplication
print(a / b);  // Division

// Negative float values
let neg = -3.5;
print(neg + 1.0);
print(neg - 1.0);
print(neg * 2.0);
print(neg / 2.0);

// Complex expressions
print((1.5 + 2.5) * 3.0);
print(10.0 / 2.0 + 3.0);
print(10.0 - 2.0 * 3.0);

// Small decimal values
print(0.1 + 0.2);
print(1.0 / 3.0);
