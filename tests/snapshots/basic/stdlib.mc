// Test standard library functions

// Math functions
print(abs(-10));
print(abs(5));
print(abs(0));

print(max(3, 7));
print(max(10, 2));
print(max(-1, -5));

print(min(3, 7));
print(min(10, 2));
print(min(-1, -5));

// String functions
print(str_len("hello"));
print(str_len(""));
print(str_len("a"));

print(str_contains("hello", "ell"));
print(str_contains("hello", "world"));
print(str_contains("hello", ""));
print(str_contains("", "a"));
